{
	
"manifest_version": 2,

"name": "Open Jira Links in New Tab",
"description": "Opens Jira Links in New Tab.",
"version": "0.2",
"content_scripts": [{
	"matches": ["https://*.atlassian.net/*"],
	"js": ["jquery.js", "background.js"]
}],

"icons": {
	"48": "icon48.png"
},

"permissions": [
	"https://*.atlassian.net/"]
}